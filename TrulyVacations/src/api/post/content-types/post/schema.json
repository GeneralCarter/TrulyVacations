{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "Title": {
      "type": "string"
    },
    "Created": {
      "type": "date"
    },
    "LastModified": {
      "type": "datetime"
    },
    "Content": {
      "type": "richtext"
    },
    "Author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
